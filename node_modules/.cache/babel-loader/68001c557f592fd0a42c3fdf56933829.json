b;AACA3B,UAAAA,KAAK,CAACK,YAAN,GAAqB,IAArB;AACApB,UAAAA,MAAM,CAAC2C,IAAP,CAAY;AAACC,YAAAA,IAAI,EAAE;AAAP,WAAZ;AACL;AACD,OApBE,EAqBFC,KArBE,CAqBI,UAAAC,GAAG,EAAI;AACT/B,QAAAA,KAAK,CAACM,WAAN,GAAoB,uBAApB;AACA0B,QAAAA,OAAO,CAACC,GAAR,CAAYF,GAAZ;AACD,OAxBD;AAyBD,KApCQ;AAqCTG,IAAAA,QArCS,oBAqCAlC,KArCA,EAqCO;AACdA,MAAAA,KAAK,CAACI,IAAN,GAAa,EAAb;AACAJ,MAAAA,KAAK,CAACK,YAAN,GAAqB,KAArB;AACApB,MAAAA,MAAM,CAAC2C,IAAP,CAAY;AAACC,QAAAA,IAAI,EAAE;AAAP,OAAZ;AACD;AAzCQ,GArBuB;AAgElCM,EAAAA,OAAO,EAAE;AACPC,IAAAA,MADO,wBACU;AAAA,UAATC,MAAS,QAATA,MAAS;AACfA,MAAAA,MAAM,CAAC,SAAD,CAAN;AACD,KAHM;AAIPC,IAAAA,OAJO,0BAIW;AAAA,UAATD,MAAS,SAATA,MAAS;AAChBA,MAAAA,MAAM,CAAC,UAAD,CAAN;AACD;AANM;AAhEyB,CAAf,CAAd","sourcesContent":["import Vue from 'vue'\nimport Vuex from 'vuex'\nimport {router} from './main.js'\n\nVue.use(Vuex)\n\nconst signUpValidationError = (signUpCredentials) => {\n  if (signUpCredentials.password !== signUpCredentials.repeatPassword) return 'Passwords donâ€˜t match'\n  if (signUpCredentials.password.length < 8) return 'Password should be at least 8 characters long'\n  let uppercasesInPassword = 0\n  let passwordContainsSpecialCharacter = false\n  for (let char in signUpCredentials.password) {\n    if (['!', '@', '#', '$', '%', '^', '&', '*'].includes(char)) passwordContainsSpecialCharacter = true\n    if (char === char.toUpperCase()) uppercasesInPassword ++\n  }\n  if (!passwordContainsSpecialCharacter) return 'Password has to contain at least one special character'\n  if (uppercasesInPassword < 2) return 'Password has to contain at least two uppercase letters'\n  return undefined\n}\n\nexport const store = new Vuex.Store({\n  state: {\n    apiUrl: 'https://limitless-badlands-68204.herokuapp.com/',\n    signUpCredentials: {\n      fullName: '',\n      email: '',\n      password: '',\n      repeatPassword: ''\n    },\n    user: {},\n    userLoggedIn: false,\n    signUpError: undefined,\n    signInError: undefined\n  },\n  getters: {\n    signUpCredentials: state => state.signUpCredentials,\n    user: state => state.user,\n    userLoggedIn: state => state.userLoggedIn,\n    signUpError: state => state.signUpError,\n    signInError: state => state.signInError\n  },\n  mutations: {\n    SET_CREDENTIALS(state, payload) {\n      state[payload.form][payload.field] = payload.value\n    },\n    SIGN_UP(state) {\n      let error = signUpValidationError(state.signUpCredentials)\n      if (error !== undefined) {\n        state.signUpError = error\n        return\n      }\n\n      fetch(state.apiUrl + 'register', {\n\t\t\t\tmethod: 'post',\n\t\t\t\theaders: {'Content-Type': 'application/json'},\n\t\t\t\tbody: JSON.stringify({\n\t\t\t\t\tname: this.state.signUpCredentials.fullName,\n\t\t\t\t\temail: this.state.signUpCredentials.email,\n\t\t\t\t\tpassword: this.state.signUpCredentials.password\n\t\t\t\t})\n\t\t\t})\n\t\t\t.then(response => response.json())\n\t\t\t.then(data => {\n\t\t\t\tif (data === 'All fields are required' ||\n            data === 'A profile with this email already exists' ||\n            data === 'There was a problem with our server') state.signUpError = data\n\t\t\t\telse {\n          state.signUpError = undefined\n          state.user = data\n          state.userLoggedIn = true\n          router.push({path: '/home'})\n\t\t\t\t}\n\t\t\t})\n\t\t\t.catch(err => {\n        state.signUpError = 'Couldnâ€™t reach server'\n        console.log(err)\n      })\n    },\n    SIGN_OUT(state) {\n      state.user = {}\n      state.userLoggedIn = false\n      router.push({path: '/'})\n    }\n  },\n  actions: {\n    signUp({commit}) {\n      commit('SIGN_UP')\n    },\n    signOut({commit}) {\n      commit('SIGN_OUT')\n    }\n  }\n})"]}]}